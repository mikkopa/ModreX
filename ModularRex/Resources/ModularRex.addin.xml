<Addin id="ModularRex" version="0.1">
  <Runtime>
    <Import assembly="ModularRex.dll"/>
  </Runtime>

  <Dependencies>
    <Addin id="OpenSim" version="0.5" />
  </Dependencies>

  <Extension path = "/OpenSim/RegionModules">
    <RegionModule id="RexAvatarUrlReciver" type="ModularRex.RexNetwork.AvatarUrlReciver" />
    <RegionModule id="RexLoginServiceInConnectorModule" type="ModularRex.RexNetwork.RexLogin.RexLoginServiceInConnectorModule" />
    <RegionModule id="RexLoginConfigurationManager" type="ModularRex.RexNetwork.RexLogin.LoginConfigurationManager" />
    <RegionModule id="EntityComponentModule" type="ModularRex.RexParts.EntityComponentModule" />
  </Extension>
</Addin>